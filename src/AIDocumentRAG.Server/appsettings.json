{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "FileManagement": {
        "DefaultSourcePath": "C:\\Documents\\Source",
        "DefaultDestinationPath": "C:\\Documents\\Copied",
        "MaxFileSizeBytes": 52428800,
        "AllowedExtensions": [ ".txt", ".md", ".pdf", ".docx", ".doc", ".rtf", ".csv", ".json", ".xml" ],
        "CreateDestinationIfNotExists": true,
        "OverwriteExistingFiles": true
    }
}